	<api-gateway:autodiscovery apiId="${autodiscovery.id}" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="3ff85fed-52dd-4040-a8e5-5d380431a46d" flowRef="swapi-request-main" />
